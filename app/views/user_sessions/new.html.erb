<div id="logo">
  <%= image_tag('logo.png', :alt => "Share all")%>
</div>
<div id="login">
  <% form_for @user_session do |f| %>
    <%= f.label :login %><br />
    <%= f.text_field :login %><br />
    <br />
    <%= f.label :password %><br />
    <%= f.password_field :password %><br />
    <br />  
    <%= f.submit "Login" %>
  <% end %>    
</div>
<div class="clear"></div>
